                        <div id="content" class="clearfix" style="text-align: center; min-height: 800px">  
                            <div style="font-size:18px; color: #FFE2AD; margin-bottom: 10px;">Карта</div>
                            <div id='map_container' style='min-height: 1095px; display: inline-block; width: 1205px'>
                                <canvas id='mapCanvas' width='1205' height='1095' style='width: 1205px; left: 50%; margin-left:-602px;'></canvas>
                                <div id='objContainer' style='width: 1205px; left: 50%; margin-left:-602px;'></div>
                                <div style="display: none;position: absolute;border-radius: 10px 10px 10px 10px; -moz-border-radius: 10px 10px 10px 10px; -webkit-border-radius: 10px 10px 10px 10px; border: 1px solid #e7a61a; padding:10px;  background: rgba(0, 0, 0, 0.5); width: 150px;" id="info">
                                    <div id="name" style="font-weight:bold; color: white"></div>
                                    <div id="owner" style="font-weight: bold"></div>
                                    <div id="description"></div>
                                </div>
                            </div>

                            <script type="text/javascript">
                                var background;
                                var backgroundLoaded = false;
                                var selectedObject = -1;

                                function IsInside(vertexes, targetPoint) {
                                    var i, j;
                                    var result = false;
                                    for (i = 0, j = vertexes.length - 1; i < vertexes.length; j = i++) {
                                        if (((vertexes[i].y > targetPoint.y) != (vertexes[j].y > targetPoint.y)) &&
                                            (targetPoint.x < (vertexes[j].x - vertexes[i].x) * (targetPoint.y - vertexes[i].y) / (vertexes[j].y - vertexes[i].y) + vertexes[i].x))
                                            result = !result;
                                    }

                                    return result;
                                }

                                function ClearCanvas() {
                                    var canvas = $("#mapCanvas");
                                    context.clearRect(0, 0, canvas.width, canvas.height);
                                }

                                function DrawArea(area, selected) {
                                    context.beginPath();
                                    context.moveTo(area.vertexes[0].x, area.vertexes[0].y);

                                    for (var i = 1; i < area.vertexes.length; i++)
                                        context.lineTo(area.vertexes[i].x, area.vertexes[i].y);

                                    context.closePath();

                                    if (selected) {
                                        context.fillStyle = area.backgroundColor;
                                        context.strokeStyle = 'green';
                                    }
                                    else {
                                        context.fillStyle = area.backgroundColor;
                                        context.strokeStyle = 'black';
                                    }

                                    context.lineWidth = 5;
                                    context.stroke();
                                    context.globalAlpha = 0.5;
                                    context.fill();
                                    context.globalAlpha = 1;
                                }

                                function DrawObject(objectDescription) {
                                    var canvas = document.getElementById("mapCanvas");

                                    if (objectDescription.location == "")
                                        $("<div style='position: absolute;' class='obj'/>")
                                             .html("<img src='" + objectDescription.image + "' style='width: " + objectDescription.size.width + "px; height: " + objectDescription.size.height + "px;'/>")
                                            .offset({ top: objectDescription.location.y - 35, left: objectDescription.location.x - 35})
                                            .mouseenter(function () {
                                                selectedObject = objectDescription;
                                                UpdateObjectInfo();
                                            })
                                            .mouseleave(function () {
                                                selectedObject = -1;
                                            })
                                            .appendTo('#objContainer')
                                    else
                                        $("<div style='position: absolute;' class='obj'/>")
                                             .html("<a href='" + objectDescription.detailMap + "'><img src='" + objectDescription.image + "' style='width: " + objectDescription.size.width + "px; height: " + objectDescription.size.height + "px;'/></a>")
                                            .offset({ top: objectDescription.location.y - 35, left: objectDescription.location.x - 35})
                                            .mouseenter(function () {
                                                selectedObject = objectDescription;
                                                UpdateObjectInfo(this);
                                            })
                                            .mouseleave(function () {
                                                selectedObject = -1;
                                            })
                                            .appendTo('#objContainer');
                                }

                                function UpdateObjectInfo(obj) {
                                    if (selectedObject == -1)
                                        return false;

                                    var canvas = document.getElementById("mapCanvas")

                                    $("#info").show();
                                    $("#name").text(selectedObject.name);
                                    $("#owner").text(selectedObject.owner);
                                    $("#owner").css('color', selectedObject.textColor);
                                    $("#description").text(selectedObject.description);

                                    var containerWidth = $("#info").width();
                                    var containerHeight = $("#info").height();

                                    var objWidth = $(obj).width();
                                    var objHeight = $(obj).height();

                                    var left = (canvas.offsetLeft + selectedObject.location.x - containerWidth / 2 - 10) + "px";
                                    var top = (canvas.offsetTop + selectedObject.location.y - objHeight - containerHeight - 10) + "px";

                                    console.log($(document).scrollTop());

                                    var div = document.getElementById("info");

                                    div.style.left = left;
                                    div.style.top = top;

                                    return true;
                                }

                                function UpdateAreaInfo(selectedArea, point) {
                                    if (selectedArea == -1) {
                                        $("#info").hide();
                                        return false;
                                    }

                                    $("#info").show();
                                    $("#name").text(areas[selectedArea].name);
                                    $("#owner").text(areas[selectedArea].owner);
                                    $("#owner").css('color', areas[selectedArea].textColor);
                                    $("#description").text("");

                                    var width = $("#info").width();
                                    var height = $("#info").height();

                                    var left = (point.x - width / 2 - 10) + "px";
                                    var top = (point.y - height - 30) + "px";

                                    var div = document.getElementById("info");

                                    div.style.left = left;
                                    div.style.top = top;

                                    return true;
                                }

                                function DrawMap(selectedPoly) {
                                    ClearCanvas();

                                    if (backgroundLoaded)
                                        context.drawImage(backgound, 0, 0);

                                    for (var i = 0; i < areas.length; i++) {
                                        if(i != selectedPoly)
                                            DrawArea(areas[i], false);
                                    }

                                    if(selectedPoly >= 0)
                                        DrawArea(areas[selectedPoly], true);
                                }

                                var context = document.getElementById('mapCanvas').getContext('2d');
                                var areas = new Array();
                                var objects = new Array();

